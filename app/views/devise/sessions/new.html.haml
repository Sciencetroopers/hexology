.row.sign-up-form-page
  .col-xs-offset-2.col-xs-8.col-md-offset-2.col-md-8
    #sign-in-form
    = simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
      .form-inputs
        = f.input :email, required: false, autofocus: true
        = f.input :password, required: false
      .form-actions.row
        .col-xs-offset-3.col-sm-offset-4
          = f.button :submit, "Let's Go!", class: "button sign-in-button"
          %p= link_to "Back", root_path, class: "button sign-up-back-button"
    -# = render "devise/shared/links"
